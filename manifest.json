{
    "name": "LinkedIn Profile Scraper",
    "version": "0.0.3",
    "manifest_version": 3,
    "permissions": [
        "tabs",
        "activeTab",
        "scripting"
    ],
    "background": {
        "service_worker": "background.js"
    },
    "action": {
        "default_popup": "popup.html",
        "default_icon": "icon.png"
    }
}
